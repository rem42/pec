{% extends "::base.html.twig" %}

{% block body %}

	<h1>Liste des compétences</h1>
	<ul>
	{% for skill in skills %}
		<li>{{ skill.skill.name }} - {{ skill.skill.skillCategory }}</li>
	{% endfor %}
	</ul>

	<h2>Ajouter une compétence</h2>

	{% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}
	{{ form(form) }}

{% endblock %}